<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:enableOnBackInvokedCallback="true"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/hatv1"
        android:label="FunctioningLogin"
        android:roundIcon="@drawable/hatv1"
        android:supportsRtl="true"
        android:theme="@style/Theme.FunctioningLogin"
        tools:targetApi="31">
        <activity
            android:name=".TestingForgotPassword"
            android:exported="true" />
        <activity
            android:name=".HomePageUIClasses.MainActivity"
            android:exported="false" />
        <activity
            android:name=".LoginUIClasses.SignupActivity"
            android:exported="true" />
        <activity
            android:name=".LoginUIClasses.LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>